<div class="section">
  <div class="blocks">
    <% section.blocks.each do |block| %>
      <%= render partial: "admin/blocks/block", locals: {f: f, block: block} %>
    <% end %>
  </div>

  <ul class="available_blocks">
    <% available_block_types.each do |block_type| %>
      <li class="<%= block_type.partial_name %>">
        <%= link_to block_type.name, new_admin_block_path(page_id: @page.id,
          section_key: section.key, type: block_type) %>
      </li>
    <% end %>
  </ul>
</div>
